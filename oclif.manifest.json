{
    "version": "1.0.0",
    "commands": {
        "update": {
            "id": "update",
            "description": "Update the typescript meta-model interface",
            "strict": true,
            "pluginName": "@blok-codes/fame-ts",
            "pluginAlias": "@blok-codes/fame-ts",
            "pluginType": "core",
            "aliases": [],
            "examples": [
                "$ oex update meta-model --source path/to/file.json\nUpdated meta-model from src to resources/pharo.json\n"
            ],
            "flags": {
                "source": {
                    "name": "source",
                    "type": "option",
                    "char": "s",
                    "default": "",
                    "description": "Path to the typescript meta-model from pharo json file",
                    "required": true,
                    "multiple": false
                }
            },
            "args": [
                {
                    "name": "options",
                    "description": "Which option to update",
                    "options": ["interface", "meta-model"],
                    "required": true
                }
            ]
        }
    }
}
