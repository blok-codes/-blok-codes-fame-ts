{
    "version": "1.0.0",
    "commands": {
        "update": {
            "id": "update",
            "description": "Update the typescript meta-model interface",
            "strict": true,
            "pluginName": "@blok-codes/fame-ts",
            "pluginAlias": "@blok-codes/fame-ts",
            "pluginType": "core",
            "aliases": [],
            "examples": [
                "$ oex update meta-model --source path/to/file.json\nUpdated meta-model from src to resources/metamodel.json\n"
            ],
            "flags": {
                "source": {
                    "name": "source",
                    "type": "option",
                    "char": "s",
                    "default": "",
                    "description": "Path to the typescript meta-model from metamodel.json file",
                    "required": true,
                    "multiple": false
                }
            },
            "args": [
                {
                    "name": "options",
                    "description": "Which option to update",
                    "options": ["interface", "meta-model"],
                    "required": true
                }
            ]
        },
        "generate": {
            "id": "generate",
            "description": "Generate the API",
            "strict": true,
            "pluginName": "@blok-codes/fame-ts",
            "pluginAlias": "@blok-codes/fame-ts",
            "pluginType": "core",
            "aliases": [],
            "examples": [
                "$ oex generate api \nGenerated the API from the meta-model to resources/generated/\n"
            ],
            "args": [
                {
                    "name": "options",
                    "description": "What to generate",
                    "options": ["api"],
                    "required": true
                }
            ]
        }
    }
}
